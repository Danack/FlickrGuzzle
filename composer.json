{
	"name": "intahwebz/flickrguzzle",
	"description": "A Flickr wrapper to allow you to call the Flickr api with Guzzle as the backend.\nGoal is to have 100% Flickr api coverage rather than just upload/display photos (currently at 23%).",
	"keywords": ["flickr", "api", "http", "rest", "web service", "curl", "client", "HTTP client"],
	"license": "MIT",

	"autoload": {
		"psr-0": {
			"Intahwebz": "src/"
		}
	},

	"authors": [
		{
			"name": "Dan Ackroyd",
			"email": "Danack@basereality.com",
			"homepage": "http://www.basereality.com"
		}
	],

	"repositories": [
		{
			"type": "vcs",
			"url": "http://github.com/Danack/guzzle"
		}
	],

	"require-dev": {
		"intahwebz/utils": "dev-master"
	},

	"require":{
		"guzzle/guzzle": ">=3.7.2"
	}

}